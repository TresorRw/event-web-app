{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    // "typeRoots": ["./node_modules/@types", "./src/types"],
    "outDir": "./dist",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    "skipLibCheck": true
  }
}
